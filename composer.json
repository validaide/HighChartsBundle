{
  "name": "validaide/highchartsbundle",
  "type": "symfony-bundle",
  "description": "HighCharts integration for your Symfony3 project.",
  "keywords": [
    "highcharts",
    "symfony",
    "bundle"
  ],
  "authors": [
    {
      "name": "Marcel Tuinstra",
      "email": "marcel.tuinstra@validaide.com"
    },
    {
      "name": "Mark Bijl",
      "email": "mark.bijl@validaide.com"
    }
  ],
  "license": [
    "MIT"
  ],
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "7.0.22"
    }
  },
  "require": {
    "php": "^5.6|^7.0",
    "symfony/framework-bundle": "^3.0",
    "symfony/dependency-injection": "~3.0",
    "symfony/options-resolver": "^3.0",
    "symfony/yaml": "^3.0",
    "symfony/templating": "^3.0",
    "symfony/console": "^3.2",
    "symfony/twig-bundle": "~3.0",
    "egeloen/json-builder": "^3.0",
    "egeloen/google-map": "^2.0",
    "egeloen/google-map-bundle": "^3.0",
    "php-http/httplug": "^1.1",
    "php-http/guzzle6-adapter": "^1.1",
    "php-http/message": "^1.6",
    "php-http/httplug-bundle": "^1.7",
    "symfony/process": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3",
    "symfony/phpunit-bridge": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "Validaide\\HighChartsBundle\\": ""
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/validaide/HighChartsBundle.git"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "0.2.0-dev"
    }
  }
}
