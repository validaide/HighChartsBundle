{
  "name": "validaide/highchartsbundle",
  "description": "HighCharts integration for your Symfony3 project.",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Marcel Tuinstra",
      "email": "marcel.tuinstra@validaide.com"
    },
    {
      "name": "Mark Bijl",
      "email": "mark.bijl@validaide.com"
    }
  ],
  "keywords": [
    "highcharts","symfony","bundle"
  ],
  "license": [
    "MIT"
  ],
  "require": {
    "php": "^5.3.3|^7.0",
    "symfony/framework-bundle": "^2.3|^3.0",
    "symfony/dependency-injection": "~2.3|~3.0",
    "symfony/options-resolver": "^2.3|^3.0",
    "symfony/yaml": "^2.3|^3.0",
    "symfony/templating": "^2.7|^3.0",
    "symfony/console": "^3.2",
    "symfony/twig-bundle": "~2.3|~3.0"
  },
  "autoload": {
    "psr-0": {
      "Validaide\\HighChartsBundle": ""
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/validaide/HighChartsBundle.git"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  }
