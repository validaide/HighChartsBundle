{
  "name": "validaide/highchartsbundle",
  "license": "MIT",
  "type": "symfony-bundle",
  "description": "HighCharts integration for your Symfony3 project.",
  "keywords": [
    "highcharts",
    "symfony",
    "bundle"
  ],
  "authors": [
    {
      "name": "Marcel Tuinstra",
      "email": "marcel.tuinstra@validaide.com"
    },
    {
      "name": "Mark Bijl",
      "email": "mark.bijl@validaide.com"
    }
  ],
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "7.4.13"
    }
  },
  "require": {
    "php": "^7.4",
    "ext-json": "*",
    "php-http/guzzle6-adapter": "^1.1",
    "php-http/httplug": "^1.1",
    "php-http/httplug-bundle": "^1.7",
    "php-http/message": "^1.6",
    "symfony/console": "^4.4",
    "symfony/dependency-injection": "^4.4",
    "symfony/framework-bundle": "^4.4",
    "symfony/options-resolver": "^4.4",
    "symfony/process": "^4.4",
    "symfony/templating": "^4.4",
    "symfony/twig-bundle": "^4.4",
    "symfony/yaml": "^4.4",
    "validaide/common": "dev-chore/symfony-4",
    "validaide/html-builder": "4.5.*"
  },
  "require-dev": {
    "brianium/paratest": "^4.0",
    "phpstan/phpstan": "^0.12.99",
    "phpunit/phpunit": "^9.0",
    "rector/rector": "^0.11.60",
    "symfony/phpunit-bridge": "^4.4"
  },
  "autoload": {
    "psr-4": {
      "Validaide\\HighChartsBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Validaide\\HighChartsBundle\\": "Tests/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/validaide/common.git",
      "no-api": true
    },
    {
      "type": "vcs",
      "url": "https://github.com/validaide/html-builder.git",
      "no-api": true
    }
  ]
}
