{
    "name": "validaide/highchartsbundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "HighCharts integration for your Symfony 4 project.",
    "keywords": [
        "highcharts",
        "symfony",
        "bundle"
    ],
    "authors": [
        {
            "name": "Marcel Tuinstra",
            "email": "marcel.tuinstra@validaide.com"
        },
        {
            "name": "Mark Bijl",
            "email": "mark.bijl@validaide.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "php-http/guzzle6-adapter": "^2.0",
        "php-http/httplug-bundle": "^1.25",
        "symfony/console": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/framework-bundle": "^4.4",
        "symfony/options-resolver": "^4.4",
        "symfony/process": "^4.4",
        "symfony/templating": "^4.4",
        "symfony/twig-bundle": "^4.4",
        "symfony/yaml": "^4.4",
        "twig/twig": "^3.0.4",
        "validaide/common": "5.8.*",
        "validaide/html-builder": "5.8.*"
    },
    "require-dev": {
        "brianium/paratest": "^6.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.15.2",
        "symfony/phpunit-bridge": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Validaide\\HighChartsBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Validaide\\HighChartsBundle\\": "Tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/validaide/common.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "https://github.com/validaide/html-builder.git",
            "no-api": true
        }
    ]
}
