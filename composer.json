{
  "name": "validaide/highchartsbundle",
  "license": "MIT",
  "type": "symfony-bundle",
  "description": "HighCharts integration for your Symfony3 project.",
  "keywords": [
    "highcharts",
    "symfony",
    "bundle"
  ],
  "authors": [
    {
      "name": "Marcel Tuinstra",
      "email": "marcel.tuinstra@validaide.com"
    },
    {
      "name": "Mark Bijl",
      "email": "mark.bijl@validaide.com"
    }
  ],
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "7.2.13"
    }
  },
  "require": {
    "php": "7.2.*",
    "ext-json": "*",
    "egeloen/json-builder": "^3.0",
    "php-http/guzzle6-adapter": "^1.1",
    "php-http/httplug": "^1.1",
    "php-http/httplug-bundle": "^1.7",
    "php-http/message": "^1.6",
    "symfony/console": "^3.4",
    "symfony/dependency-injection": "~3.4",
    "symfony/framework-bundle": "^3.4",
    "symfony/options-resolver": "^3.4",
    "symfony/process": "^3.4",
    "symfony/templating": "^3.4",
    "symfony/twig-bundle": "~3.4",
    "symfony/yaml": "^3.4",
    "validaide/html-builder": "2.3.0"
  },
  "require-dev": {
    "brianium/paratest": "^3.0",
    "phpunit/phpunit": "^8.0",
    "symfony/phpunit-bridge": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "Validaide\\HighChartsBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Validaide\\HighChartsBundle\\": "Tests/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/validaide/common.git",
      "no-api": true
    },
    {
      "type": "vcs",
      "url": "https://github.com/validaide/html-builder.git",
      "no-api": true
    }
  ]
}
